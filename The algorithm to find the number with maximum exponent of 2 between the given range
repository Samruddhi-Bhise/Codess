def countExponents(i):
    count = 0
    while i % 2 == 0 and i != 0:
        count += 1
        i = i // 2
    return count

def maxExponents(a, b):
    maximum, number = 0, a
    for i in range(a, b + 1):  # Make it inclusive of b
        temp = countExponents(i)
        if temp > maximum:
            maximum, number = temp, i
        elif temp == maximum:
            number = min(number, i)  # Optional: keeps smaller number if same exponent
    return number

# Input
a, b = map(int, input().split())
print(maxExponents(a, b))
